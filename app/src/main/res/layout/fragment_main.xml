<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.y2793623b.map_fotos_pro.MainActivityFragment"
    tools:showIn="@layout/activity_main">


    <org.osmdroid.views.MapView
        android:layout_width="332dp"
        android:layout_height="440dp"
        android:id="@+id/mapView"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="50dp"
        android:layout_marginStart="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginRight="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:text="Video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_video"
        android:layout_marginStart="55dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="15dp"
        app:layout_constraintTop_toBottomOf="@+id/mapView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintVertical_bias="0.32999998"
        android:layout_below="@+id/mapView"
        android:layout_alignLeft="@+id/mapView"
        android:layout_alignStart="@+id/mapView"
        android:layout_marginTop="1dp" />

    <Button
        android:text="Foto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/bt_foto"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mapView"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="29dp"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="10dp"
        android:layout_alignTop="@+id/bt_video"
        android:layout_alignRight="@+id/mapView"
        android:layout_alignEnd="@+id/mapView" />


</android.support.constraint.ConstraintLayout>
